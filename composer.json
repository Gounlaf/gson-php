        }
    ],
    "require": {
        "php": ">= 7.0",
        "doctrine/annotations": "^1.2",
        "tebru/enum": "^0.3.0",
        "tebru/realtype": "^0.1.2",
        "guzzlehttp/psr7": "^1.0",
        "tebru/php-collections": "0.2.*",
        "symfony/cache": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
